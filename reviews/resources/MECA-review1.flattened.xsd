  <!-- COUNTS MODEL -->
  <!-- Content model for the <counts> element. -->
  <!-- COUNTS -->
  <!--
    Wrapper element to hold all metadata that
    "counts how many of something appear in the
    article
    Details at:
    http://jats.nlm.nih.gov/archiving/tag-library/1.1/index.html?elem=counts
